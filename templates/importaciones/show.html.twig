{% extends 'base.html.twig' %}

{% block title %}Importaciones{% endblock %}

{% block body %}
    <h1>Importaciones</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ importacione.id }}</td>
            </tr>
            <tr>
                <th>Fecha_importado</th>
                <td>{{ importacione.fechaImportado ? importacione.fechaImportado|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fecha_inicio_plan</th>
                <td>{{ importacione.fechaInicioPlan }}</td>
            </tr>
            <tr>
                <th>Fecha_fin_plan</th>
                <td>{{ importacione.fechaFinPlan }}</td>
            </tr>
            <tr>
                <th>Ciclo</th>
                <td>{{ importacione.ciclo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('importaciones_index') }}">back to list</a>

    <a href="{{ path('importaciones_edit', {'id': importacione.id}) }}">edit</a>

    {{ include('importaciones/_delete_form.html.twig') }}
{% endblock %}
